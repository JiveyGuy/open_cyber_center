{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": [
      "dist/",
      "index.html"
    ],
    "withGlobalTauri": false
  },
  "package": {
    "productName": "occ",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.occ.dev",
      "targets": "all"
    },
    "security": {
      "csp": "default-src 'self' blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline'; script-src-elem 'self' https://accounts.google.com 'unsafe-eval' 'unsafe-hashes' 'unsafe-inline'; style-src 'self' https://accounts.google.com 'unsafe-eval' 'unsafe-hashes' 'unsafe-inline';"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": true,
        "resizable": false,
        "title": "occ"
      },
      {
        "fullscreen": true,
        "resizable": false,
        "decorations": false,
        "url": "https://geekprank.com/matrix-code-rain/",
        "label": "splashscreen"
      }
    ]
  }
}
